generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  secureId String @unique
  email    String @unique
  name     String
  password String
}

model units {
  id        Int    @id @default(autoincrement())
  secureId  String @unique
  unitOrder Int
  name      String
}
