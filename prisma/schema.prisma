generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  secureId String  @unique
  username String  @unique
  password String?
  mmr      Int
}

model units {
  id        Int    @id @default(autoincrement())
  secureId  String @unique
  unitOrder Int
  name      String
}
